<ul id="proj-tabs" class="tabs tabbed">
<% for(var j = 0; j < type.length; j++) { %>
  <li class="tab col s2"><a href="#tab<%= j%>"><%= type[j]%></a></li>
<%}%>
</ul>
<% for(var j = 0; j < type.length; j++) { %>
  <div class="row" id="tab<%= j%>">
    <%if(j > 0) {
%>
    <div class="col s12">
      <%- include('_lab-filters')%>
    </div>
<%}%>
<% for(var i = 0; i < projs.length; i++) { %>

        <div class="col s12 m6 l4">
          <div class="card sticky-action">
          <div class="card-content" style="padding:0px">
            <div class="project-border"></div>
            <div class="bookmark right">
              <a class="waves-effect" href="#"><i class="fa fa-star-o"></i></a>
            </div>
            <div class="proj-content">
              <span class="card-title"><%= projs[i].lang%></span>
              <p><%= projs[i].title%></p>
              <p><%= projs[i].created_on%></p>
            </div>
          </div>
          <div class="card-action">
            <a href="#">open</a>
          </div>
        </div>
        </div>
<%} %>
  </div>
<%}%>
